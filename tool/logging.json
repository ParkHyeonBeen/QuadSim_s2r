{
  "version": 1,
  "formatters": {
    "simple": {
    "format": "[%(levelname)s] %(asctime)s > %(message)s"
    }
  },

  "handlers": {
    "console_stream": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "simple",
      "stream": "ext://sys.stdout"
    }
  },

  "root": {
      "level": "DEBUG",
      "handlers": ["console_stream"]
    }
}

